	"scripts": {
		"test": "gulp clean && tsc && gulp copy && mocha --file ./dist/test/mocha/config.js --timeout 30000 --exit ./dist/test/**/*.spec.js",
		"testGeneric": "gulp clean && tsc && gulp copy && mocha --file ./dist/test/mocha/config.js --timeout 30000 --exit ./dist/test/generic/**/*.spec.js",
		"testServices": "gulp clean && tsc && gulp copy && mocha --file ./dist/test/mocha/config.js --timeout 90000 --exit ./dist/test/services/**/*.service.spec.js",
		"testUtilities": "gulp clean && tsc && gulp copy && mocha --file ./dist/test/mocha/config.js --timeout 90000 --exit ./dist/test/utils/**/*.utility.spec.js",
		"testIntegrations": "gulp clean && tsc && gulp copy && mocha --file ./dist/test/mocha/config.js --timeout 90000 --exit ./dist/test/integrations/**/*.spec.js",
		"testDatabase": "gulp clean && tsc && gulp copy && mocha --file ./dist/test/mocha/config.js --timeout 30000 --exit ./dist/test/database/**/*.spec.js",
		"start": "yarn rsBuild && node ./dist/app.js",
		"rsBuild": "gulp clean && tsc && gulp copy",
		"table": "tsc && node ./dist/scripts/createTable.js",
		"hash": "tsc && node ./dist/scripts/hashPass.js",
		"flushUsers": "tsc && node ./dist/scripts/flushRedisUserCache.js",
		"prettier:check": "prettier --check ./src/**/*.{ts,tsx,js,jsx,html,scss,css,md,json}",
		"prettier:fix-all": "prettier --write ./src/**/*.{ts,tsx,js,jsx,html,scss,css,md,json}",
		"deploy": "node ./dist/scripts/deploy.js"
	},
